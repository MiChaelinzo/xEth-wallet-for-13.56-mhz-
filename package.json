{
  "name": "ethereum-wallet-api-example",
  "version": "0.0.1",
  "description": "a basic example of an API for managing an ethereum wallet",
  "main": "dist",
  "engines": {
    "node": "11.x",
    "npm": "6.13.x"
  },
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src",
    "debug": "npm run prestart && node --nolazy --inspect-brk=9229 dist"
  },
  "repository": "https://git.heroku.com/xeth.git",
  "author": "Omodaka9375",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.3",
    "compression": "^1.5.2",
    "express": "^4.21.1",
    "helmet": "^3.23.3",
    "morgan": "^1.9.1",
    "scrypt": "^6.0.3",
    "web3": "^4.13.0",
    "winston": "^2.4.2",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^9.12.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.3.0",
    "nodemon": "^3.1.7",
    "prettier": "^1.12.1"
  }
}
